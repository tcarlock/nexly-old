<li>
	<div id="review-btns-#{review.id}" class="btns-tab btns-tab-small review-btns">
		<%= render :partial => 'reviews/admin_buttons', :locals => { :review => review } %>
	</div>
	<div class="rating-icon">
		<%= review.rating %>
	</div>
	<p>
		<%= review.details %>
	</p>
	<span class="footer">Posted by <%= review.user.display_name %> on <%= review.created_at.to_s(:feed_date)%></span>
</li>