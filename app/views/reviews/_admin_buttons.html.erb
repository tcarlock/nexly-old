<%= link_to "", new_business_review_response_path(review.business, review), :class => "review-response-link", :title => 'Submit review response', :id => "dispute-review-" + review.id.to_s %>
<% if !review.is_under_review %>
	<%= form_tag dispute_business_review_path(review.business, review), :remote => true do |f| %>
		<a class="control-link dispute-review-link" title="Dispute review"></a>
	<% end %>
<% end %>
<% if !review.is_approved %>
	<%= form_tag approve_business_review_path(review.business, review), :remote => true do |f| %>
		<a class="control-link approve-review-link" title="Approve this review"></a>
	<% end %>
<% end %>