<li id='review-request-<%= review_request.id %>' class="review-request">
	<% if show_admin %>
		<div class="btns-tab btns-tab-small review-btns">
			<%= render :partial => 'review_requests/admin_buttons', :locals => { :review_request => review_request } %>
		</div>
	<% end %>
	<p>
		<%= review_request.message %>
	</p>
	<div class="footer">
		Sent to <%= review_request.email %> on <%= review_request.created_at.to_s(:feed_date_time)%>
	</div>
</li>