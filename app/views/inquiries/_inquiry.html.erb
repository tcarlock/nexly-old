<li id="biz-inquiry-<%= inquiry.id %>" class="biz-inquiry">
	<% if show_admin && !inquiry.is_archived %>
		<div class="btns-tab btns-tab-small review-btns">
			<%= render :partial => 'inquiries/admin_buttons', :locals => { :inquiry => inquiry } %>
		</div>
	<% end %>
	<p>
		<%= inquiry.details %>
	</p>
	<div class="footer">
		Sent by <%= inquiry.name %> on <%= inquiry.created_at.to_s(:feed_date_time)%>
	</div>
</li>